{
  "name": "@duckdb-wasm-adapter/svelte",
  "version": "1.0.0",
  "description": "Svelte adapter for DuckDB WASM",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo node_modules tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@duckdb-wasm-adapter/core": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/svelte": "^4.1.0",
    "@types/node": "^20.0.0",
    "svelte": "^4.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0",
    "jsdom": "^26.0.0"
  },
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "keywords": [
    "duckdb",
    "wasm",
    "database",
    "sql",
    "svelte",
    "adapter"
  ],
  "license": "MIT"
}