{
  "name": "@northprint/duckdb-wasm-adapter-core",
  "version": "0.1.0",
  "description": "Core library for DuckDB WASM adapter",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist .turbo node_modules tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.28.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^1.2.0",
    "apache-arrow": "17.0.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@duckdb/duckdb-wasm": "^1.28.0"
  },
  "keywords": [
    "duckdb",
    "wasm",
    "database",
    "sql",
    "adapter"
  ],
  "author": "Northprint",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northprint/duckdb-wasm-adapter-component.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/northprint/duckdb-wasm-adapter-component/issues"
  },
  "homepage": "https://github.com/northprint/duckdb-wasm-adapter-component#readme",
  "publishConfig": {
    "access": "public"
  }
}